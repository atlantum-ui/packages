{
    "extends": "@nrwl/workspace/presets/npm.json",
    "npmScope": "atlantum",
    "affected": {
        "defaultBase": "main"
    },
    "cli": {
        "defaultCollection": "@nrwl/react"
    },
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/workspace/tasks-runners/default",
            "options": {
                "cacheableOperations": ["build", "lint", "test", "e2e"]
            }
        }
    },
    "generators": {
        "@nrwl/react": {
            "application": {
                "babel": true
            }
        }
    }
}
